{
  "name": "aeolus-checker",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "minify:css": "npx clean-css-cli ./public/styles.css -o ./public/styles.min.css",
    "obfuscate:js": "npx javascript-obfuscator ./public/script.js -o ./public/script.min.js --string-array true --string-array-encoding base64 --compact true",
    "build": "npm run minify:css && npm run obfuscate:js",
    "start": "node index.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "body-parser": "^1.19.0"
  },
  "devDependencies": {
    "javascript-obfuscator": "^4.1.0",
    "clean-css-cli": "^5.6.2"
  }
}